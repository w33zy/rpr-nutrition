{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rpr/nutrition",
	"version": "1.0.0",
	"title": "Nutrition",
	"category": "widgets",
	"icon": "smiley",
	"description": "The recipe nutrition block",
	"keywords": [
		"nutrition",
		"recipepress"
	],
	"attributes": {
		"rpr_recipe_calorific_value": { "type": "number", "default": 0 },
		"rpr_recipe_fat": { "type": "number", "default": 0 },
		"rpr_recipe_carbohydrate": { "type": "number", "default": 0 }
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "rpr-nutrition",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
